<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d %thread %level %logger - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="jp.co.rikinet.eventloop" level="debug" />

    <root level="info">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
